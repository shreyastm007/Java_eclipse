package com.thoughtfocus;

import java.util.Scanner;

public class Loops {
	public void gender() {
		Scanner scan = new Scanner(System.in);
		String s=scan.nextLine();
		System.out.println(s);
//		String s="female";
		
		
		if(s=="male") {
			System.out.println("MALE");
		}
		else if(s=="female") {
			System.out.println("FEMALE");
		}
		else{
			System.out.println("OTHERS");
		}
		
	}

}
