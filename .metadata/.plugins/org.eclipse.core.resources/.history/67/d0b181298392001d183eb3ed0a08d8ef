package com.thoughtfocus;
import java.util.*;  
public class LinkedExample {

	public void example(){  
	  
	  //LinkedList<String> linked=new LinkedList<String>();  
		LinkedList linked=new LinkedList();
	  linked.add("Ravi");  
	  linked.add("Vijay");  
	  linked.add("Ravi");  
	  linked.add("Ajay");  
	  linked.add(10);
	  
	//deletion
		System.out.println("The deleted element is:" + linked.remove(2));
		System.out.println(linked);

		// Fetching the element at the head of the Stack
				System.out.println("The element at the top of the stack is:" + linked.peek());
				
				
		//Searching:
		//It returns the position of the element from the top of the stack. Else, it returns -1.
		System.out.println( linked.contains("Grapes"));
		System.out.println( linked.contains("100"));
		
		System.out.println( linked.isEmpty());
		System.out.println( linked.size());
		
		
		//Iterate means to fetch the elements of the stack. 
		//We can fetch elements of the stack using three different methods are as follows:
		//Using iterator() Method
		//Using forEach() Method
		//Using listIterator() Method
		
		System.out.println("Iteration:");
		linked.forEach(n ->  
		{  
		System.out.println(n);  
		});  
		
}  
	  
	
}

