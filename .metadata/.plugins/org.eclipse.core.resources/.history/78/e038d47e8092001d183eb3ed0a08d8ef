package com.thoughtfocus;
import java.util.*;  
public class ArayLst {
	
	 public void example(){  
	  //ArrayList<String> list=new ArrayList<String>();
		 ArrayList list=new ArrayList();
	      list.add("Mango");//Adding object in arraylist    
	      list.add("Apple");    
	      list.add("Banana");    
	      list.add("Grapes");   
	      list.add(10); 
	      list.add(1000); 
	      list.add(1); 	      
	      
	      //Printing the arraylist object   
	      System.out.println(list);  
	      
	      
	   // Fetching the element at the head of the Stack
			//System.out.println("The element at the top of the stack is:" + list.peek());
			
			//deletion
			System.out.println("The deleted element is:" + list.remove(0));
			System.out.println(list);

			//Searching:
			//It returns the position of the element from the top of the stack. Else, it returns -1.
			System.out.println( list.contains("Grapes"));
			System.out.println( list.contains("100"));
			
			System.out.println( list.isEmpty());
			System.out.println( list.size());
			
			
			//Iterate means to fetch the elements of the stack. 
			//We can fetch elements of the stack using three different methods are as follows:
			//Using iterator() Method
			//Using forEach() Method
			//Using listIterator() Method
			
			System.out.println("Iteration:");
			list.forEach(n ->  
			{  
			System.out.println(n);  
			});  
			
	 }  
}
